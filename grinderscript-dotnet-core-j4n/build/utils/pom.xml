<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>net.grinderscript.dotnet</groupId>
		<artifactId>grinderscript-dotnet-parent</artifactId>
		<version>1.0-SNAPSHOT</version>
		<relativePath>../../..</relativePath>
	</parent>
	<artifactId>grinderscript-dotnet-core-j4-build-utils</artifactId>
	<packaging>pom</packaging>
	<name>GrinderScript.Net :: Java To .Net Bridge build utilities</name>
	<description>GrinderScript.Net Java To .Net Bridge build utilities</description>
	<profiles>
		<profile>
			<id>install-jni4net</id>
			<build>
				<plugins>
					<plugin>
						<artifactId>maven-install-plugin</artifactId>
						<executions>
							<execution>
								<id>install-jni4net.j</id>
								<phase>generate-sources</phase>
								<goals>
									<goal>install-file</goal>
								</goals>
								<configuration>
									<artifactId>jni4net.j</artifactId>
									<packaging>jar</packaging>
									<file>${project.basedir}/../../external/jni4net/lib/jni4net.j-${jni4net.version}.jar</file>
								</configuration>
							</execution>
							<execution>
								<id>install-jni4net.n</id>
								<phase>generate-sources</phase>
								<goals>
									<goal>install-file</goal>
								</goals>
								<configuration>
									<artifactId>jni4net.n</artifactId>
									<file>${project.basedir}/../../external/jni4net/lib/jni4net.n-${jni4net.version}.dll</file>
								</configuration>
							</execution>
							<execution>
								<id>install-jni4net.n.w32.v20</id>
								<phase>generate-sources</phase>
								<goals>
									<goal>install-file</goal>
								</goals>
								<configuration>
									<artifactId>jni4net.n.w32.v20</artifactId>
									<file>${project.basedir}/../../external/jni4net/lib/jni4net.n.w32.v20-${jni4net.version}.dll</file>
								</configuration>
							</execution>
							<execution>
								<id>install-jni4net.n.w32.v40</id>
								<phase>generate-sources</phase>
								<goals>
									<goal>install-file</goal>
								</goals>
								<configuration>
									<artifactId>jni4net.n.w32.v40</artifactId>
									<file>${project.basedir}/../../external/jni4net/lib/jni4net.n.w32.v40-${jni4net.version}.dll</file>
								</configuration>
							</execution>
							<execution>
								<id>install-jni4net.n.w64.v20</id>
								<phase>generate-sources</phase>
								<goals>
									<goal>install-file</goal>
								</goals>
								<configuration>
									<artifactId>jni4net.n.w64.v20</artifactId>
									<file>${project.basedir}/../../external/jni4net/lib/jni4net.n.w64.v20-${jni4net.version}.dll</file>
								</configuration>
							</execution>
							<execution>
								<id>install-jni4net.n.w64.v40</id>
								<phase>generate-sources</phase>
								<goals>
									<goal>install-file</goal>
								</goals>
								<configuration>
									<artifactId>jni4net.n.w64.v40</artifactId>
									<file>${project.basedir}/../../external/jni4net/lib/jni4net.n.w64.v40-${jni4net.version}.dll</file>
								</configuration>
							</execution>
						</executions>
						<configuration>
							<groupId>net.sf.jni4net</groupId>
							<version>${jni4net.version}</version>
							<packaging>dll</packaging>
						</configuration>
					</plugin>
				</plugins>
			</build>
		</profile>
		<profile>
			<id>deploy-jni4net</id>
			<build>
				<plugins>
					<plugin>
						<artifactId>maven-deploy-plugin</artifactId>
						<executions>
							<execution>
								<id>deploy-jni4net.j</id>
								<phase>generate-sources</phase>
								<goals>
									<goal>deploy-file</goal>
								</goals>
								<configuration>
									<artifactId>jni4net.j</artifactId>
									<packaging>jar</packaging>
									<file>${project.basedir}/../../external/jni4net/lib/jni4net.j-${jni4net.version}.jar</file>
								</configuration>
							</execution>
							<execution>
								<id>deploy-jni4net.n</id>
								<phase>generate-sources</phase>
								<goals>
									<goal>deploy-file</goal>
								</goals>
								<configuration>
									<artifactId>jni4net.n</artifactId>
									<file>${project.basedir}/../../external/jni4net/lib/jni4net.n-${jni4net.version}.dll</file>
								</configuration>
							</execution>
							<execution>
								<id>deploy-jni4net.n.w32.v20</id>
								<phase>generate-sources</phase>
								<goals>
									<goal>deploy-file</goal>
								</goals>
								<configuration>
									<artifactId>jni4net.n.w32.v20</artifactId>
									<file>${project.basedir}/../../external/jni4net/lib/jni4net.n.w32.v20-${jni4net.version}.dll</file>
								</configuration>
							</execution>
							<execution>
								<id>deploy-jni4net.n.w32.v40</id>
								<phase>generate-sources</phase>
								<goals>
									<goal>deploy-file</goal>
								</goals>
								<configuration>
									<artifactId>jni4net.n.w32.v40</artifactId>
									<file>${project.basedir}/../../external/jni4net/lib/jni4net.n.w32.v40-${jni4net.version}.dll</file>
								</configuration>
							</execution>
							<execution>
								<id>deploy-jni4net.n.w64.v20</id>
								<phase>generate-sources</phase>
								<goals>
									<goal>deploy-file</goal>
								</goals>
								<configuration>
									<artifactId>jni4net.n.w64.v20</artifactId>
									<file>${project.basedir}/../../external/jni4net/lib/jni4net.n.w64.v20-${jni4net.version}.dll</file>
								</configuration>
							</execution>
							<execution>
								<id>deploy-jni4net.n.w64.v40</id>
								<phase>generate-sources</phase>
								<goals>
									<goal>deploy-file</goal>
								</goals>
								<configuration>
									<artifactId>jni4net.n.w64.v40</artifactId>
									<file>${project.basedir}/../../external/jni4net/lib/jni4net.n.w64.v40-${jni4net.version}.dll</file>
								</configuration>
							</execution>
						</executions>
						<configuration>
							<groupId>net.sf.jni4net</groupId>
							<version>${jni4net.version}</version>
							<packaging>dll</packaging>
							<repositoryId>ext-release-local</repositoryId>
							<url>http://localhost:8081/artifactory/ext-release-local</url>
						</configuration>
					</plugin>
				</plugins>
			</build>
		</profile>
	</profiles>
</project>
